<h1>Listing jewelleries</h1>

<table>
  <thead>
    <tr>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @jewelleries.each do |jewellery| %>
      <tr>
        <td><%= link_to 'Show', jewellery %></td>
        <td><%= link_to 'Edit', edit_jewellery_path(jewellery) %></td>
        <td><%= link_to 'Destroy', jewellery, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Jewellery', new_jewellery_path %>
